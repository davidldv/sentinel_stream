# Used by docker-compose for variable substitution and passed into the agent container.

OPENAI_API_KEY=

GEMINI_API_KEY=

# Optional: choose provider/model for the agent
LLM_PROVIDER=gemini
LLM_MODEL=gemini-3-flash-preview

# Reduce LLM calls (seconds between analyses). Helps avoid rate/quota bursts.
AGENT_MIN_SECONDS_BETWEEN_LLM_CALLS=10
